{
  "name": "@mapperai/annotated-scene",
  "version": "0.0.0",
  "description": "Mapper.ai Annotated Scene",
  "scripts": {
    "dev": "npm run lint-watch & p1=$!; npm run typecheck-watch & p2=$!; wait $p1 $p2",
    "typecheck": "./node_modules/.bin/tsc -p ./tsconfig.json --noEmit",
    "typecheck-watch": "npm run typecheck -- -w",
    "lint": "npm run lint-js && npm run lint-ts",
    "lint-watch": "npm run lint-js -- --watch & $p5=$!; npm run lint-ts -- --watch & p6=$!; wait $p5 $p6",
    "lint-js": "esw --format codeframe --no-eslintrc --config ./.eslintrc.js './**/{.,}*.{js,json}'",
    "lint-ts": "esw --format codeframe --config ./src/.eslintrc.js 'src/**/*.{ts,tsx}'",
    "lint-fix": "npm run lint-js -- --fix; npm run lint-ts -- --fix"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/Signafy/annotated-scene"
  },
  "devDependencies": {
    "@types/lodash": "4.14.106",
    "@types/react": "16.0.27",
    "@types/stats.js": "0.17.0",
    "@types/three": "0.84.34",
    "@types/uuid": "3.4.3",
    "babel-eslint": "^8.2.6",
    "eslint": "5.2.0",
    "eslint-config-standard": "11.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.1.0",
    "eslint-plugin-html": "4.0.5",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-typescript": "0.12.0",
    "eslint-plugin-vue": "4.4.0",
    "eslint-watch": "^4.0.2",
    "typescript": "2.9.1",
    "typescript-eslint-parser": "17.0.1"
  },
  "dependencies": {
    "@mapperai/mapper-models": "1.9.8",
    "@mapperai/mapper-saffron-sdk": "1.0.3",
    "animation-loop": "1.4.0",
    "events": "^3.0.0",
    "google-protobuf": "3.5.0",
    "immutable": "3.8.2",
    "lodash": "4.17.4",
    "react": "16.2.0",
    "react-redux": "5.0.7",
    "react-resize-observer": "1.0.3",
    "redux": "3.7.2",
    "reselect": "3.0.1",
    "sprintf-js": "1.0.3",
    "stats.js": "0.17.0",
    "three": "0.84.0",
    "three-obj-loader": "1.1.2",
    "typedux": "0.1.20",
    "typelogger": "1.0.29",
    "utm": "1.1.1",
    "uuid": "3.1.0"
  }
}
