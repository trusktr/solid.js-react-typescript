{
  "name": "@mapperai/mapper-annotator",
  "appName": "Annotator",
  "appID": "Annotator",
  "version": "1.0.0",
  "description": "Mapper.ai Point Cloud Annotator",
  "htmlEntry": "dist/app/browser-entry.html",
  "main-off": "dist/app/annotator-entry-ui.js",
  "scripts": {
    "lint": "./node_modules/.bin/tslint \"src/**/*.ts\" --force",
    "start": "./node_modules/.bin/electron bin/annotator-start.js",
    "build": "./etc/scripts/compile.js",
    "watch": "./etc/scripts/compile-watch.js",
    "dev": "npm run build; npm run watch & P1=$!; npm start & P2=$!; wait $P1 $P2",
    "postinstall": "npm install --no-save grpc@1.7.3 --runtime=electron --target=1.7.9"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "__TS_CONFIG__": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react"
      }
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src\\/.*\\.spec\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/etc/tests/file-mock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/chrome": "0.0.42",
    "@types/dat.gui": "0.6.0",
    "@types/electron-window-state": "2.0.31",
    "@types/image-size": "0.0.29",
    "@types/jest": "19.2.2",
    "@types/jquery": "2.0.33",
    "@types/lodash": "4.14.106",
    "@types/mkdirp": "0.5.2",
    "@types/node": "8.0.54",
    "@types/react": "16.0.27",
    "@types/react-dom": "0.14.23",
    "@types/react-modal": "3.1.2",
    "@types/stats.js": "0.17.0",
    "@types/three": "0.84.34",
    "@types/uuid": "3.4.3",
    "@types/zmq": "0.0.22",
    "awesome-typescript-loader": "3.0.8",
    "babel-cli": "6.11.4",
    "babel-core": "6.13.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-runtime": "6.8.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2016-node5": "1.1.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.8.0",
    "circular-dependency-plugin": "2.0.0",
    "css-loader": "0.27.3",
    "electron": "1.7.9",
    "electron-rebuild": "1.6.0",
    "electron-unhandled": "1.1.0",
    "electron-window-state": "4.1.1",
    "eslint": "3.2.2",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.17.0",
    "jest": "19.0.2",
    "loader-utils": "1.1.0",
    "node-sass": "4.9.0",
    "pug": "2.0.0-beta6",
    "pug-loader": "2.3.0",
    "sass-loader": "6.0.3",
    "semver": "5.3.0",
