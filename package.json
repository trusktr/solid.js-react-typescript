{
  "name": "@mapperai/mapper-annotator",
  "appName": "Annotator",
  "appID": "Annotator",
  "version": "1.9.0",
  "description": "Mapper.ai HD Map Editor",
  "appIcon": "https://s3.amazonaws.com/mapper-website-assets/saffron/icons/Annotator_Icon.svg",
  "main": "dist/package/saffron.js",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/Signafy/mapper-annotator"
  },
  "scripts": {
    "test": "npm run lint-all && npm run prettier-check && npm run build && echo 'implement tests'",
    "dev": "(rm -rf dist; npm run build:dev:watch & p1=$!; npm run electron & p2=$!; wait $p1 $p2)",
    "electron": "electron ./src/annotator/annotator-entry-main/index.js",
    "browser": "./scripts/serve.js",
    "build": "npm run build:dev",
    "build:prod": "rm -rf dist && npm run build:saffron:prod && npm run build:annotator:prod",
    "build:dev": "rm -rf dist && npm run build:saffron:dev && npm run build:annotator:dev",
    "build:dev:watch": "npm run build:saffron:dev -- --watch --progress & p1=$!; npm run build:annotator:dev -- --watch --progress & p2=$!; wait $p1 $p2",
    "build:saffron:prod": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --max_old_space_size=4096 --config etc/webpack/webpack.config.saffron.js --mode production",
    "build:annotator:prod": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --max_old_space_size=4096 --config etc/webpack/webpack.config.annotator.js --mode production",
    "build:saffron:dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --max_old_space_size=4096 --config etc/webpack/webpack.config.saffron.js --mode development",
    "build:annotator:dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --max_old_space_size=4096 --config etc/webpack/webpack.config.annotator.js --mode development",
    "package": "npm run build && ./build-scripts/build.js",
    "deploy:web:dev": "npm run build:dev && aws s3 cp --acl public-read dist/package/ s3://mapper-public-website-staging/annotator/ --recursive",
    "deploy:web:prod": "npm run build:prod && aws s3 cp --acl public-read dist/package/ s3://mapper.ai/annotator/ --recursive",
    "COMMENT about typecheck:": " the '&& :' makes it so specific files are not passed to typecheck in the below lint-staged invocation, otherwise it will have an error.",
    "typecheck": "tsc -p ./tsconfig.json --noEmit && :",
    "typecheck-watch": "tsc -p ./tsconfig.json --noEmit --watch",
    "electron-rebuild": "electron-rebuild",
    "prettier": "npm run prettier-js -- --write && npm run prettier-ts -- --write && npm run prettier-md -- --write",
    "prettier-list": "npm run --silent prettier-js-list && npm run --silent prettier-ts-list && npm run --silent prettier-md-list",
    "prettier-js": "prettier '{,!(node_modules)/**/}*.{js,json}'",
    "prettier-ts": "prettier '{,!(node_modules)/**/}*.{ts,tsx}'",
    "prettier-md": "prettier '{,!(node_modules)/**/}*.md'",
    "prettier-js-list": "npm run prettier-js -- --list-different || true",
    "prettier-ts-list": "npm run prettier-ts -- --list-different || true",
    "prettier-md-list": "npm run prettier-md -- --list-different || true",
    "prettier-check": "npm run --silent prettier-js-check && npm run --silent prettier-ts-check && npm run --silent prettier-md-check",
    "prettier-js-check": "npm run prettier-js -- --check",
    "prettier-ts-check": "npm run prettier-ts -- --check",
    "prettier-md-check": "npm run prettier-md -- --check",
    "lint": "esw --format codeframe --config ./.eslintrc.js",
    "lint-fix": "npm run lint -- --fix",
    "lint-all": "npm run lint-js && npm run lint-ts",
    "lint-fix-all": "npm run lint-js -- --fix && npm run lint-ts -- --fix",
    "lint-watch": "npm run lint-js -- --watch & $p5=$!; npm run lint-ts -- --watch & p6=$!; wait $p5 $p6",
    "lint-js": "npm run lint -- './**/{.,}*.{js,json,html}'",
    "lint-ts": "npm run lint -- './**/*.{ts,tsx}'",
    "COMMENT about dev-or-master": "exits non-zero if we're not on develop or master branches",
    "dev-or-master": "git branch | grep \\* | cut -d ' ' -f2 | grep -E 'develop|master' > /dev/null",
    "version-patch": "node scripts/version.js patch",
    "version-minor": "node scripts/version.js minor",
    "version-major": "node scripts/version.js major"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run typecheck"
    ],
    "*.{js,ts,tsx,json}": [
      "npm run lint-fix -- $@",
      "prettier --write $@",
      "git add"
    ],
    "*.{html}": [
      "npm run lint-fix -- $@",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/auth0-lock": "^11.4.12",
    "@types/dat.gui": "0.7.1",
    "@types/electron-window-state": "2.0.33",
    "@types/jquery": "2.0.33",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "4.14.106",
    "@types/mkdirp": "0.5.2",
