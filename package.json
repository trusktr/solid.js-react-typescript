    "@types/react-modal": "3.1.2",
    "@types/react-redux": "^5.0.7",
    "@types/serve-handler": "^6.1.0",
    "@types/sprintf-js": "1.1.1",
    "@types/uuid": "3.4.3",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "8.2.6",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron-rebuild": "^1.8.5",
    "eslint": "5.2.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.1.0",
    "eslint-plugin-html": "4.0.5",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-typescript": "0.12.0",
    "eslint-watch": "4.0.2",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.3",
    "husky": "^1.0.0-rc.13",
    "json-loader": "^0.5.7",
    "lint-staged": "^8.0.0-beta.2",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "shelljs": "^0.8.3",
    "simple-git": "^1.107.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.22.1",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "typescript-eslint-parser": "20.1.1",
    "url-loader": "^1.0.1",
    "webpack": "4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-node-externals": "^1.7.2",
    "webpack-sources": "1.1.0",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@mapperai/mapper-annotated-scene": "4.0.0",
    "@mapperai/mapper-models": "1.9.8",
    "@mapperai/mapper-saffron-sdk": "2.1.9",
    "@mapperai/mapper-themes": "^2.1.0",
    "@material-ui/core": "^3.9.3",
    "@types/react-beautiful-dnd": "^11.0.2",
    "animation-loop": "1.4.0",
    "async-file": "2.0.2",
    "auth0-lock": "^11.17.2",
    "core-js": "^2.6.5",
    "css-modules-require-hook": "4.2.3",
    "dat.gui": "0.7.1",
    "electron": "^5.0.0",
    "electron-unhandled": "1.1.0",
    "electron-window-state": "5.0.2",
    "events": "^3.0.0",
    "events-typed": "^2.0.0",
    "executable": "4.1.1",
