{
  "name": "@mapperai/mapper-annotator",
  "appName": "Annotator",
  "appID": "Annotator",
  "version": "1.0.0",
  "description": "Mapper.ai Point Cloud Annotator",
  "main": "TODO dist/app/annotator-entry-ui.js",
  "scripts": {
    "start": "electron src/annotator/annotator-entry-main/index.js",
    "dev": "npm run lint-watch & p1=$!; npm run typecheck-watch & p2=$!; npm start & p3=$!; wait $p1 $p2 $p3",
    "typecheck": "./node_modules/.bin/tsc -p ./tsconfig.json --noEmit",
    "typecheck-watch": "npm run typecheck -- -w",
    "postinstall": "npm install --no-save grpc@1.7.3 --runtime=electron --target=1.7.9",
    "lint": "npm run lint-js && npm run lint-ts",
    "lint-watch": "npm run lint-js -- --watch & $p5=$!; npm run lint-ts -- --watch & p6=$!; wait $p5 $p6",
    "lint-js": "esw --format codeframe '{src,etc}/**/*.{js,json}' '.eslintrc*' 'package.json'",
    "lint-ts": "esw --format codeframe '{src,etc}/**/*.{ts,tsx}'",
    "lint-fix": "npm run lint-js -- --fix; npm run lint-ts -- --fix"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "__TS_CONFIG__": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react"
      }
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src\\/.*\\.spec\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/etc/tests/file-mock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/bluebird": "2.0.32",
