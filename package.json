		"eslint-config-prettier": "^3.1.0",
		"eslint-friendly-formatter": "4.0.1",
		"eslint-loader": "2.1.0",
		"eslint-plugin-html": "4.0.5",
		"eslint-plugin-json": "1.2.0",
		"eslint-plugin-promise": "4.0.1",
		"eslint-plugin-typescript": "0.12.0",
		"eslint-watch": "4.0.2",
		"file-loader": "^1.1.11",
