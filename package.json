    "tslint": "5.8.0",
    "typescript": "2.4.1",
    "webpack": "2.2.1",
    "webpack-node-externals": "1.2.0"
  },
  "dependencies": {
    "@mapperai/mapper-models": "1.9.8",
    "animation-loop": "1.3.0",
    "async-file": "2.0.2",
    "dat.gui": "0.7.1",
    "executable": "4.1.1",
    "google-protobuf": "3.5.0",
    "image-size": "0.6.2",
    "immutable": "3.8.2",
